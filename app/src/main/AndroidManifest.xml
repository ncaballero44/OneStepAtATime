<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onestepatatime">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".ClientMainActivity"></activity>
        <activity android:name=".ClientCalendarActivity"></activity>
        <activity android:name=".ClientJournalActivity"></activity>
        <activity android:name=".ClientWorksheetsActivity"></activity>
        <activity android:name=".ClientNotesActivity"></activity>
        <activity android:name=".ClientTherapistListActivity"></activity>
        <activity android:name=".ClientEmergencyContactListActivity"></activity>

        <activity android:name=".TherapistMainActivity"></activity>
        <activity android:name=".TherapistCalendarActivity"></activity>
        <activity android:name=".TherapistAssessmentActivity"></activity>
        <activity android:name=".TherapistWorksheetsActivity"></activity>
        <activity android:name=".TherapistNotesActivity"></activity>
        <activity android:name=".TherapistClientListActivity"></activity>

        <activity android:name=".MainActivity"></activity>

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>